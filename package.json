{
	"name": "circle-backend",
	"version": "1.0.0",
	"description": "backend server for circle",
	"main": "dist/index.js",
	"private": true,
	"scripts": {
		"test": "jest --watch",
		"test:coverage": "jest --coverage",
		"dev": "ts-node-dev --project tsconfig.server.json --respawn src/index.ts",
		"build": "next build && rimraf dist && tsc --project tsconfig.server.json",
		"prettify": "prettier --write 'src/**/*.ts'",
		"start": "cross-env NODE_ENV=production node dist/index.js"
	},
	"repository": {
		"type": "git",
