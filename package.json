	"bugs": {
		"url": "https://gitlab.com/timothy1996/circle-backend/issues"
	},
	"homepage": "https://gitlab.com/timothy1996/circle-backend#readme",
	"devDependencies": {
		"@types/bcryptjs": "2.4.2",
		"@types/cors": "2.8.6",
		"@types/express": "4.17.6",
		"@types/jest": "25.2.3",
		"@types/node": "14.0.5",
		"@types/nodemailer": "6.4.0",
		"@types/react": "16.9.35",
		"@types/react-dom": "16.9.8",
		"cross-env": "7.0.2",
